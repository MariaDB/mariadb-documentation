  <svg width="640" height="640" viewBox="0 60 640 700">
    <defs>
      <marker markerHeight="5" markerWidth="5" orient="auto" viewBox="-200 -50 100 100" id="arr">
        <path d="m-100,0l-100,-40l30,40l-30,40z"/>
      </marker>
      <path d="m-16,0l-15,-10l0,5l-20,0l0,10l20,0l0,5l15,-10" id="larr"/>
      <use xlink:href="#larr" transform="rotate(180)" id="rarr"/>
    </defs>
    <g stroke-width="3" stroke="black">
      <line marker-end="url(#arr)" x1="350" y1="430" x2="350" y2="370"/>
