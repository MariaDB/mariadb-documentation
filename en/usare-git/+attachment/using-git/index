<section>
  <h2>Common operations</h2>
</section>

<section>
  <h3>Examine the history</h3>
  <ul>
    <li>git log
    <li>git log --graph
    <li>git log -G
    <li>git show
  </ul>
</section>

<section>
  <h3>Referencing a commit</h3>
  <ul>
    <li>dae86e1950b1277e545cee180551750029cfe735
    <li>HEAD, @
    <li>tag name, branch name
    <li>@{u}, branch@{u}
    <li>foo^ (HEAD^, dae86e^, tag^, @{u}^, ...)
    <li>foo~2, foo~3, foo~10, ...
    <li>:/regex, foo^{/regex}
  </ul>
</section>

<section>
  <h3>HEAD^^3~2^2</h3>
  <svg width="640" height="640" viewBox="0 60 640 700">
    <defs>
      <marker markerHeight="5" markerWidth="5" orient="auto" viewBox="-200 -50 100 100" id="arr">
        <path d="m-100,0l-100,-40l30,40l-30,40z"/>
      </marker>
      <path d="m-16,0l-15,-10l0,5l-20,0l0,10l20,0l0,5l15,-10" id="larr"/>
      <use xlink:href="#larr" transform="rotate(180)" id="rarr"/>
    </defs>
    <g stroke-width="3" stroke="black">
      <line marker-end="url(#arr)" x1="450" y1="430" x2="450" y2="370"/>
