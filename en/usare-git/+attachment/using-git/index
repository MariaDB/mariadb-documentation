</section>

<section>
  <h3>Ranges</h3>
  <ul>
    <li>rev
    <li>rev1..rev2
    <li>rev1...rev2
  </ul>
  <ul>
    <li>^rev
    <li>rev^@
    <li>rev^!
  </ul>
</section>

<section>
  <h3>red..blue</h3>
  <svg width="640" height="640" viewBox="0 60 640 700">
    <defs>
      <marker markerHeight="5" markerWidth="5" orient="auto" viewBox="-200 -50 100 100" id="arr">
        <path d="m-100,0l-100,-40l30,40l-30,40z"/>
      </marker>
      <path d="m-16,0l-15,-10l0,5l-20,0l0,10l20,0l0,5l15,-10" id="larr"/>
      <use xlink:href="#larr" transform="rotate(180)" id="rarr"/>
    </defs>
    <g stroke-width="3" stroke="black">
      <line marker-end="url(#arr)" x1="350" y1="400" x2="350" y2="340"/>
      <line marker-end="url(#arr)" x1="350" y1="340" x2="350" y2="280"/>
      <line marker-end="url(#arr)" x1="350" y1="280" x2="350" y2="220"/>
      <line marker-end="url(#arr)" x1="350" y1="220" x2="350" y2="160"/>
      <line marker-end="url(#arr)" x1="350" y1="160" x2="350" y2="100"/>

