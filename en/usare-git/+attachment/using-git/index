      <line marker-end="url(#arr)" x1="350" y1="280" x2="350" y2="220"/>
      <line marker-end="url(#arr)" x1="350" y1="220" x2="350" y2="160"/>
      <line marker-end="url(#arr)" x1="250" y1="430" x2="250" y2="370"/>
      <line marker-end="url(#arr)" x1="250" y1="370" x2="250" y2="310"/>
      <line marker-end="url(#arr)" x1="250" y1="310" x2="250" y2="250"/>

      <line marker-end="url(#arr)" x1="250" y1="250" x2="350" y2="220"/>
